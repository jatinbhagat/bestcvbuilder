{
  "component_weights": {
    "structure": 25,
    "keywords": 20,
    "contact": 15,
    "formatting": 20,
    "achievements": 10,
    "readability": 10
  },
  "score_categories": {
    "excellent": {
      "min_score": 90,
      "max_score": 100,
      "label": "Excellent",
      "description": "Outstanding ATS compatibility - your resume is highly optimized!",
      "letter_grade": "A",
      "interview_rate": 15,
      "color": "green"
    },
    "very_good": {
      "min_score": 80,
      "max_score": 89,
      "label": "Very Good", 
      "description": "Very good ATS compatibility with room for minor improvements",
      "letter_grade": "B",
      "interview_rate": 12,
      "color": "blue"
    },
    "good": {
      "min_score": 70,
      "max_score": 79,
      "label": "Good",
      "description": "Good ATS compatibility - some optimization recommended",
      "letter_grade": "C",
      "interview_rate": 8,
      "color": "yellow"
    },
    "fair": {
      "min_score": 60,
      "max_score": 69,
      "label": "Fair",
      "description": "Fair ATS compatibility - significant improvements needed",
      "letter_grade": "D",
      "interview_rate": 5,
      "color": "orange"
    },
    "poor": {
      "min_score": 0,
      "max_score": 59,
      "label": "Poor",
      "description": "Poor ATS compatibility - major optimization required",
      "letter_grade": "F",
      "interview_rate": 2,
      "color": "red"
    }
  },
  "interview_rate_mapping": {
    "90": 15,
    "85": 12,
    "80": 10,
    "75": 8,
    "70": 6,
    "65": 5,
    "60": 4,
    "55": 3,
    "50": 2,
    "0": 2
  },
  "essential_sections": {
    "required": [
      "experience", "work experience", "professional experience", "employment",
      "education", "academic background", "qualifications",
      "skills", "technical skills", "core competencies", "expertise"
    ],
    "recommended": [
      "summary", "professional summary", "profile", "objective",
      "projects", "key projects", "notable projects",
      "certifications", "licenses", "credentials",
      "achievements", "accomplishments", "awards"
    ],
    "optional": [
      "publications", "research", "patents",
      "volunteer", "volunteering", "community service",
      "languages", "language skills",
      "interests", "hobbies", "personal interests",
      "references", "professional references"
    ]
  },
  "contact_requirements": {
    "essential": {
      "email": {
        "weight": 5,
        "patterns": [
          "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
          "\\b[A-Za-z0-9._%+-]+\\s*@\\s*[A-Za-z0-9.-]+\\s*\\.\\s*[A-Z|a-z]{2,}\\b",
          "[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}"
        ],
        "validation": "professional_email_preferred"
      },
      "phone": {
        "weight": 4,
        "patterns": [
          "\\(?\\d{3}\\)?[-.\\ ]?\\d{3}[-.\\ ]?\\d{4}",
          "\\+\\d{1,3}[-.\\ ]?\\d{3}[-.\\ ]?\\d{3}[-.\\ ]?\\d{4}",
          "\\+\\d{1,3}[-.\\ ]?\\(?\\d{3}\\)?[-.\\ ]?\\d{3}[-.\\ ]?\\d{4}",
          "\\d{3}[-.\\ ]\\d{3}[-.\\ ]\\d{4}",
          "\\(?\\d{3}\\)?\\s?\\d{3}[-.\\ ]?\\d{4}",
          "\\+\\d{1,3}\\s?\\(?\\d{3}\\)?\\s?\\d{3}[-.\\ ]?\\d{4}"
        ],
        "validation": "valid_format_required"
      }
    },
    "recommended": {
      "linkedin": {
        "weight": 4,
        "patterns": ["linkedin\\.com/in/[\\w-]+"],
        "validation": "professional_profile_preferred"
      },
      "location": {
        "weight": 2,
        "patterns": ["[A-Za-z\\s]+,\\s*[A-Za-z]{2}", "[A-Za-z\\s]+\\s+\\d{5}"],
        "validation": "city_state_or_zip"
      }
    },
    "optional": {
      "portfolio": {
        "weight": 2,
        "patterns": ["https?://[\\w.-]+", "www\\.[\\w.-]+"],
        "validation": "valid_url"
      },
      "github": {
        "weight": 2,
        "patterns": ["github\\.com/[\\w-]+"],
        "validation": "for_technical_roles"
      }
    }
  },
  "formatting_guidelines": {
    "file_types": {
      "preferred": ["pdf", "docx", "doc"],
      "accepted": ["txt", "rtf"],
      "avoid": ["jpg", "png", "gif", "html"]
    },
    "fonts": {
      "ats_friendly": [
        "Arial", "Calibri", "Times New Roman", "Helvetica", 
        "Georgia", "Verdana", "Tahoma", "Trebuchet MS"
      ],
      "avoid": [
        "Comic Sans", "Papyrus", "Impact", "Brush Script", "Chalkduster"
      ]
    },
    "formatting_rules": {
      "use_standard_headers": true,
      "avoid_tables": true,
      "avoid_text_boxes": true,
      "avoid_columns": true,
      "use_bullet_points": true,
      "consistent_formatting": true,
      "adequate_white_space": true
    },
    "length_guidelines": {
      "entry_level": {"min": 1, "max": 1, "pages": 1},
      "experienced": {"min": 1, "max": 2, "pages": 2}, 
      "executive": {"min": 2, "max": 3, "pages": 3},
      "academic": {"min": 2, "max": 10, "pages": "variable"}
    }
  },
  "keyword_analysis": {
    "density_thresholds": {
      "optimal_min": 0.02,
      "optimal_max": 0.08,
      "warning_threshold": 0.1,
      "spam_threshold": 0.15
    },
    "matching_strategies": {
      "exact_match": {"weight": 1.0, "case_sensitive": false},
      "partial_match": {"weight": 0.7, "min_length": 4},
      "synonym_match": {"weight": 0.8, "use_wordnet": true},
      "stem_match": {"weight": 0.6, "use_stemming": true}
    },
    "industry_detection": {
      "confidence_threshold": 0.6,
      "min_keywords_required": 3,
      "weight_recent_experience": true,
      "fallback_to_general": true
    }
  },
  "achievement_scoring": {
    "quantification_bonus": 2,
    "impact_verb_bonus": 1,
    "result_oriented_bonus": 1,
    "specificity_bonus": 1,
    "min_achievements_recommended": 3,
    "max_achievements_analyzed": 10
  }
}