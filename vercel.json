{
  "framework": "vite", 
  "buildCommand": "rm -rf dist node_modules/.vite && npm ci && npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}